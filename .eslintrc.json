{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "suitable-style-guide",
  "rules": {
      "indent": ["error", suitable-indentation],
      "linebreak-style": ["error", suitable-linebreak-style],
      "quotes": ["error", suitable-quote-style],
      "semi": ["error", suitable-semi-style]
    }
}